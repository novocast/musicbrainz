{
  "name": "novocast/musicbrainz",
  "description": "Wrapper for Musicbrainz API v2",
  "keywords": [
    "musicbrainz",
    "api",
    "artists",
    "tracks",
    "albums",
    "isrc"
  ],
  "homepage": "http://github.com/novocast/musicbrainz",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Anthony Totton",
      "email": "novocast7@gmail.com",
      "homepage": "http://anthonytotton.com"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "makinacorpus/php-lucene": "^1.0",
    "guzzlehttp/guzzle": "^6.5"
  },
  "autoload": {
    "psr-4": {
      "MusicBrainz\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MusicBrainz\\": [
        "src/",
        "tests/"
      ]
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.2"
  },
  "scripts": {
    "test": [
      "phpunit"
    ]
  }
}
